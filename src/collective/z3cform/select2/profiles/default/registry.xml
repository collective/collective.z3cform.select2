<?xml version="1.0"?>
<registry>

  <!-- bundles definition -->
  <records prefix="plone.bundles/collective-z3cform-select2"
           interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="depends">plone</value>
    <value key="enabled">True</value>
    <value key="csscompilation">++resource++select2-widget/select2-widget.css</value>
    <value key="jscompilation">++resource++select2-widget/select2-widget.js</value>
    <value key="load_async">False</value>
    <value key="load_defer">False</value>
  </records>

  <records prefix="plone.bundles/collective-z3cform-select2-faceted"
           interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="depends">faceted.view</value>
    <value key="enabled">True</value>
    <value key="jscompilation">++resource++faceted.select2.view.js</value>
    <value key="load_async">True</value>
    <value key="load_defer">True</value>
  </records>

</registry>
